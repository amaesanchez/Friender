{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "password": {
        "type": "string",
        "minLength": 5,
        "maxLength": 20
      },
      "interests": {
        "type": "string",
        "minLength": 1,
        "maxLength": 300
      },
      "hobbies": {
        "type": "string",
        "minLength": 1,
        "maxLength": 300
      },
      "images": {
        "type": "string"
      },
      "location": {
        "type": "number"
      },
      "radius": {
        "type": "number"
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "password",
      "images",
      "location",
      "radius"
    ]
}
